#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int a,b;
    cin>>a;
    cin>>b;
    for(int i=0;i<a;i++)
    {
        b++;
    }
    cout<<b;
    return 0;
}
